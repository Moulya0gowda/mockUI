{
    "compilerOptions": {
      "target": "ESNext",
      "useDefineForClassFields": true,
      "lib": ["DOM","ESNext"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }